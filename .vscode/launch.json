{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch Debug",
      "preLaunchTask": "Build (Debug)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/ski",
      "args": [
        "../ski-programs/test.ski",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}/build",
      "environment": [
        {
          "name": "GLOG_logtostderr",
          "value": "1"
        }
      ],
      "externalConsole": false,
      "MIMode": "lldb",
      "sourceFileMap": {
        ".": "${workspaceFolder}"
      }
    },
  ]
}
